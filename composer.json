{
    "name": "iahunter/php7-telecom-management",
    "description": "Telecom Management Server",
    "keywords": ["telecom", "laravel"],
    "license": "LGPL3.0",
    "type": "project",
    "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/metaclassing/adLDAP.git"
      },
      {
          "type": "vcs",
          "url": "https://github.com/iahunter/PHP5-CallManager-AXL.git"
      }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "dingo/api": "^1.0",
        "metaclassing/utility": "dev-master",
        "phpseclib/phpseclib": "2.0.*",
        "tymon/jwt-auth": "^1.0",
		"adldap/adldap": "4.0.4.x-dev",
        "silber/bouncer": "^1.0",
        "darkaonline/l5-swagger": "~3.0",
        "beberlei/assert": "^2.6",
        "owen-it/laravel-auditing": "^3.0",
        "spatie/laravel-backup": "^4.0.0",
        "spatie/laravel-activitylog": "^1.12",
        "metaclassing/php7-sshwrapper": "dev-master",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
		"iahunter/PHP5-CallManager-AXL": "dev-master",
        "geerlingguy/ping": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
	"minimum-stability" : "dev",
	"prefer-stable" : true
}
